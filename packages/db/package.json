{
  "name": "db",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./schema": {
      "types": "./lib/schema.d.ts",
      "default": "./lib/schema.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "dev": "rimraf lib && tsc --watch",
    "generate": "dotenvx run -- drizzle-kit generate",
    "migrate": "dotenvx run -- drizzle-kit migrate",
    "drop": "dotenvx run -- drizzle-kit drop",
    "push": "dotenvx run -- drizzle-kit push",
    "build": "rimraf lib && tsc",
    "build:migration": "rimraf dist && ncc build src/migration.ts -o dist && cpy ./migrations ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "drizzle-orm": "0.36.4",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.24.5",
    "@types/pg": "^8.11.10",
    "@vercel/ncc": "^0.38.3",
    "cpy-cli": "^5.0.0",
    "drizzle-kit": "^0.28.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  }
}
