{
  "name": "sdk",
  "version": "0.1.0",
  "description": "SDK library generated by Nestia",
  "main": "lib/sdk/index.js",
  "typings": "lib/sdk/index.d.ts",
  "scripts": {
    "build": "npm run build:sdk && npm run compile",
    "build:sdk": "rimraf ../../apps/backend/src/sdk/functional && cd ../../apps/backend && npx nestia sdk && cd ../../packages/sdk",
    "compile": "rimraf lib && tsc",
    "deploy": "npm run build && npm publish",
    "prepare": "ts-patch install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@nestia/fetcher": "^6.0.6",
    "typia": "^9.3.1"
  }
}
